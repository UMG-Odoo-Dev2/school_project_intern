<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="subject_view_tree" model="ir.ui.view">
        <field name="name">subject.view.tree</field>
        <field name="model">school.subject</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="subject_view_form" model="ir.ui.view">
        <field name="name">subject.view.form</field>
        <field name="model">school.subject</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" required="True"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="subject_create_action" model="ir.actions.act_window">
        <field name="name">Subject Fields</field>
        <field name="res_model">school.subject</field>
        <field name="view_mode">tree,form</field>
    </record>
   
</odoo>
