<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="subject_view_form" model="ir.ui.view">
        <field name="name">subject.view.form</field>
        <field name="model">subject.model</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="subject"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="subject_view_tree" model="ir.ui.view">
        <field name="name">subject.view.tree</field>
        <field name="model">subject.model</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="subject"/>
            </tree>
        </field>
    </record>


    <record id="subject_action" model="ir.actions.act_window">
        <field name="name">Subject Action</field>
        <field name="res_model">subject.model</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html"> -->
            
        <!-- </field> -->
    </record>

</odoo>

